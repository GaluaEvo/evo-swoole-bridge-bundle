{
    "name": "galuaevo/evo-swoole-bridge-bundle",
    "description": "Swoole Bridge For Php Frameworks",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "fuong",
            "email": "insidestyles@gmail.com"
        }
    ],
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/galuaevo/evo-swoole-bridge"
        }
    ],

    "require": {
        "php": "^7.2|^8.0",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0|^6.0",
        "galuaevo/evo-swoole-bridge": "^0.6.0",
        "symfony/psr-http-message-bridge": "^1.0|^2.0",
        "symfony/console": "^3.0|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^9.0",
        "eaglewu/swoole-ide-helper": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Insidestyles\\SwooleBridgeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "Tests/DependencyInjection"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
